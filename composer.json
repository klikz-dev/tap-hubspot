    "keywords": ["template"],
    "homepage": "https://www.bytespree.com",
    "license": "Proprietary",
    "require": {
        "datamanagementinc/singer-php": "^1.0",
        "php": ">=8.1",
        "hubspot/api-client": "^9.2"
